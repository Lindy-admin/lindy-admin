<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Date</th>
      <th>Label</th>
      <th>Template id</th>
      <th>Status</th>
    </tr>
  </thead>

  <tbody>
    <% mailings.each do |mailing| %>
      <tr>
        <td><%= mailing.created_at %></td>
        <td><%= mailing.label %></td>
        <td><%= mailing.remote_template_id %></td>
        <td><%= mailing.status %></td>
      </tr>
    <% end %>
  </tbody>
</table>
